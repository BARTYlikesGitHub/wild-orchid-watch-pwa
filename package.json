{
  "name": "wild-orchid-watch-pwa",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && rollup -c --failAfterWarnings && workbox injectManifest",
    "test:unit": "vue-cli-service test:unit",
    "analyse-bundle": "yarn build && webpack-bundle-analyzer dist/stats.json",
    "build:serve": "BROWSERSLIST_ENV=debug DISABLE_MINIFY=1 VUE_APP_FORCE_VUE_DEVTOOLS=1 yarn build && servor dist/ index.html --no-browser",
    "build:serve:watch": "nodemon --ext vue,js,mjs,json --delay 2 --watch src --watch sw-src --watch package.json --exec bash -c 'yarn build:serve'",
    "firebase:deploy:ci": "doit() { firebase --non-interactive --token ${FIREBASE_TOKEN:?} deploy --only hosting:${1:?}; }; doit",
    "lint": "vue-cli-service lint src/ tests/ vue-config/ sw-src/",
    "lint:specified": "vue-cli-service lint",
    "lint:check": "yarn lint --no-fix --max-warnings 0",
    "lint:check:scripts": "vue-cli-service lint --no-fix --max-warnings 0 scripts/",
    "serve:debug": "BROWSERSLIST_ENV=debug vue-cli-service serve",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/vue-cli-service test:unit --runInBand"
  },
  "dependencies": {
    "@sentry/browser": "^6.17.9",
    "@sentry/integrations": "^6.17.9",
    "base64-js": "^1.3.1",
    "comlink": "4.2.0",
    "dayjs": "^1.8.26",
    "dms2dec": "^1.1.0",
    "elliptic": "^6.5.4",
    "exif-js": "git+https://github.com/ternandsparrow/exif-js.git#gh-63-outside-bounds",
    "exif-reader": "^1.0.3",
    "fetch-retry": "^2.2.3",
    "formdata-polyfill": "https://github.com/ternandsparrow/FormData#safari13.1-attempt2-installable",
    "fuse.js": "^5.0.10-beta",
    "gmap-vue": "^3.5.2",
    "highlight.js": "^10.4.1",
    "http-proxy": "^1.18.1",
    "ini": "^1.3.7",
    "is-svg": "^4.2.2",
    "is_js": "^0.9.0",
    "jwt-decode": "^2.2.0",
    "localforage": "ternandsparrow/localForage#gh-910",
    "lodash": "^4.17.11",
    "node-notifier": "^8.0.1",
    "onsenui": "^2.12.0",
    "pkce-challenge": "^3.0.0",
    "pwacompat": "^2.0.9",
    "register-service-worker": "^1.6.2",
    "sass": "^1.21.0",
    "smoothscroll-polyfill": "^0.4.4",
    "sockjs": "^0.3.20",
    "stackdriver-error-reporting-clientside-js-client": "https://github.com/ternandsparrow/stackdriver-error-reporting-clientside-js-client#v0.1.0",
    "uuid": "^3.3.3",
    "vue": "^2",
    "vue-analytics": "^5.17.2",
    "vue-head": "^2.1.1",
    "vue-onsenui": "^2.7.0",
    "vue-router": "3.0.7",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^3.2.0",
    "workbox-background-sync": "^6.5.3",
    "workbox-cli": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "y18n": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@rollup/plugin-alias": "^3.1.1",
    "@sentry/webpack-plugin": "^1.8.0",
    "@types/lodash": "^4.14.132",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-pwa": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@vue/vue2-jest": "^27",
    "babel-jest": "^27",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-import-meta": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "fake-indexeddb": "^3.1.7",
    "favicons": "^6",
    "favicons-webpack-plugin": "^5.0.2",
    "firebase-tools": "^6.10.0",
    "jest": "^27.1.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.33.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "sass-loader": "^13.0.0",
    "servor": "^2.2.1",
    "vue-template-compiler": "^2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": {
    "production": [
      "ChromeAndroid >= 60",
      "FirefoxAndroid >= 55",
      "iOS >= 11.3",
      "safari >= 11.1",
      "samsung >= 8.2"
    ],
    "debug": [
      "last 2 Chrome versions"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "mjs"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue2-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(jsx?|mjs)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "setupFiles": [
      "fake-indexeddb/auto"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/unit/setup.js"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "readme": "README.md"
}
